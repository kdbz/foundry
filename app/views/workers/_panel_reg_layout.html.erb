		</section><! --/wrapper -->
      </section><!-- /MAIN CONTENT -->

      <!--main content end-->
      <!--footer start-->
      <footer class="footer">
          <div class="text-center">
              2014 - Stanford HCI Group
              <!--
<a href="#" class="go-top">
                  <i class="fa fa-angle-up"></i>
              </a>
-->
          </div>
      </footer>
      <!--footer end-->
